[visual] # Visualizer filter

# Effects list (字串)
#effect-list=spectrum

# Video width (整數)
#effect-width=800

# Video height (整數)
#effect-height=500

# Show 80 bands instead of 20 (布林)
#visual-80-bands=1

# Draw peaks in the analyzer (布林)
#visual-peaks=1

# Enable original graphic spectrum (布林)
#spect-show-original=0

# Draw the base of the bands (布林)
#spect-show-base=1

# Base pixel radius (整數)
#spect-radius=42

# Spectral sections (整數)
#spect-sections=3

# V-plane color (整數)
#spect-color=80

# Draw bands in the spectrometer (布林)
#spect-show-bands=1

# Show 80 bands instead of 20 (布林)
#spect-80-bands=1

# Number of blank pixels between bands. (整數)
#spect-separ=1

# Amplification (整數)
#spect-amp=8

# Draw peaks in the analyzer (布林)
#spect-show-peaks=1

# Peak extra width (整數)
#spect-peak-width=61

# Peak height (整數)
#spect-peak-height=1

